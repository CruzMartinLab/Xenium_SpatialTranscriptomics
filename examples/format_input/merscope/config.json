{
    "workflow": {
      "threads": 12,
      "seed": 1,
      "gitpath": "/path/to/punkst",
      "rawdir" : "/path/to/raw/data",
      "compressed" : 1,
      "datadir": "/path/to/output",
      "tmpdir": "/path/to/store/temporary/files",
      "tilesize": 500,
      "hexgrids": [12],
      "topics": [12, 24],
      "pixhex": 12,
      "nmove": 3,
      "res": 0.5,
      "nucleiradius": 3,
      "scale": 0.5
    },
    "job": {
      "time": "12:00:00",
      "cpus-per-task": 12,
      "mem": "8G",
      "job-name": "vz",
      "output": "/path/to/logs/%x.%j.out",
      "error":  "/path/to/logs/%x.%j.err",
      "extra_lines" : "# python venv\nsource /path/to/venv/bin/activate\n"
    }
  }
